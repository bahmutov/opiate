{
  "name": "opiate",
  "version": "1.0.1",
  "description": "Curried, immediate version of dependency injection by argument name.",
  "main": "opiate.js",
  "dependencies": {
    "heroin": "0.5.0",
    "lazy-ass": "0.6.0"
  },
  "devDependencies": {
    "commitizen": "2.4.1",
    "cz-conventional-changelog": "1.1.4",
    "mocha": "2.3.3",
    "pre-git": "1.0.0",
    "webpack": "1.12.3"
  },
  "scripts": {
    "test": "mocha -R spec *-spec.js",
    "build": "webpack opiate.js dist/opiate.js --display-modules",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/opiate.git"
  },
  "keywords": [
    "injection",
    "dependency",
    "partial",
    "application",
    "selective"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/opiate/issues"
  },
  "homepage": "https://github.com/bahmutov/opiate#readme",
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test",
        "npm version"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  }
}
